{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"]
      }
    ],
    ["styled-jsx/babel", { "optimizeForSpeed": true }]
  ]
}
